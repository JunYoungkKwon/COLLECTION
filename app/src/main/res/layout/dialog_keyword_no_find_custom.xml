<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.cardview.widget.CardView
        android:id="@+id/dialog_text_bg_cv"
        android:layout_width="296dp"
        android:layout_height="176dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:cardCornerRadius="10dp"
        app:cardElevation="0dp">

        <View
            android:id="@+id/dialog_text_bg_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/bottom_navi">
        </View>

        <TextView
            android:id="@+id/dialog_text_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/suit_regular"
            android:textStyle="normal"
            android:textSize="14sp"
            android:layout_marginTop="60dp"
            android:textColor="@color/dark_taupe"
            android:letterSpacing="-0.01"
            android:layout_gravity="center_horizontal"
            android:text="@string/dialog_keyword_no_find_text"/>
    </androidx.cardview.widget.CardView>

    <ImageView
        android:id="@+id/dialog_top_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_padding_indicator1"
        android:layout_marginTop="28dp"
        app:layout_constraintTop_toTopOf="@+id/dialog_text_bg_cv"
        app:layout_constraintStart_toStartOf="@+id/dialog_text_bg_cv"
        app:layout_constraintEnd_toEndOf="@+id/dialog_text_bg_cv"/>

    <ImageButton
        android:id="@+id/dialog_cancle_ib"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:src="@drawable/goto_login_btn_event"
        android:background="@color/transparent"
        app:layout_constraintStart_toStartOf="@+id/dialog_text_bg_cv"
        app:layout_constraintEnd_toEndOf="@+id/dialog_text_bg_cv"
        app:layout_constraintBottom_toBottomOf="@+id/dialog_text_bg_cv" />


</androidx.constraintlayout.widget.ConstraintLayout>