<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/img_bg_matchpage"
    android:orientation="vertical"
    >

    <ImageView
        android:id="@+id/match_main_ic"
        android:layout_width="270dp"
        android:layout_height="380dp"
        android:src="@drawable/ic_match_main"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="435dp"/>


    <View
        android:id="@+id/match_color_text_background_v"
        android:layout_width="168dp"
        android:layout_height="15dp"
        android:background="@color/light_peach"
        app:layout_constraintStart_toStartOf="@id/match_main_ic"
        app:layout_constraintTop_toBottomOf="@id/match_main_ic"
        android:layout_marginTop="113dp"
        >
    </View>

    <ImageView
        android:id="@+id/match_color_text_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_match_color_text"
        app:layout_constraintStart_toStartOf="@id/match_main_ic"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="307dp"

        />

    <View
        android:id="@+id/match_background_horizontalline_color_v"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="@color/light_peach"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/match_color_text_iv"
        android:layout_marginTop="17.5dp">
    </View>


    <View
        android:id="@+id/match_weather_text_background_v"
        android:layout_width="232dp"
        android:layout_height="15dp"
        android:background="@color/light_peach"
        app:layout_constraintStart_toStartOf="@id/match_main_ic"
        app:layout_constraintTop_toBottomOf="@id/match_main_ic"
        android:layout_marginTop="182dp"
        >
    </View>

    <ImageView
        android:id="@+id/match_weather_text_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_match_weather_text"
        app:layout_constraintStart_toStartOf="@id/match_main_ic"
        app:layout_constraintTop_toBottomOf="@id/match_background_horizontalline_color_v"
        android:layout_marginTop="17.5dp"

        />

    <View
        android:id="@+id/match_background_horizontalline_weather_v"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="@color/light_peach"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/match_weather_text_iv"
        android:layout_marginTop="17.5dp">
    </View>

    <View
        android:id="@+id/match_place_text_background_v"
        android:layout_width="163dp"
        android:layout_height="15dp"
        android:background="@color/light_peach"
        app:layout_constraintStart_toStartOf="@id/match_main_ic"
        app:layout_constraintTop_toBottomOf="@id/match_main_ic"
        android:layout_marginTop="251dp"
        >
    </View>

    <ImageView
        android:id="@+id/match_place_text_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_match_place_text"
        app:layout_constraintStart_toStartOf="@id/match_main_ic"
        app:layout_constraintTop_toBottomOf="@id/match_background_horizontalline_weather_v"
        android:layout_marginTop="17.5dp"

        />

    <View
        android:id="@+id/match_background_horizontalline_place_v"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="@color/light_peach"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/match_place_text_iv"
        android:layout_marginTop="17.5dp">
    </View>

    <View
        android:id="@+id/match_who_text_background_v"
        android:layout_width="111dp"
        android:layout_height="15dp"
        android:background="@color/light_peach"
        app:layout_constraintStart_toStartOf="@id/match_main_ic"
        app:layout_constraintTop_toBottomOf="@id/match_main_ic"
        android:layout_marginTop="320dp"
        >
    </View>

    <ImageView
        android:id="@+id/match_who_text_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_match_who_text"
        app:layout_constraintStart_toStartOf="@id/match_main_ic"
        app:layout_constraintTop_toBottomOf="@id/match_background_horizontalline_place_v"
        android:layout_marginTop="17.5dp"

        />

    <View
        android:id="@+id/match_background_horizontalline_who_v"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="@color/light_peach"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/match_who_text_iv"
        android:layout_marginTop="17.5dp">
    </View>




</androidx.constraintlayout.widget.ConstraintLayout>