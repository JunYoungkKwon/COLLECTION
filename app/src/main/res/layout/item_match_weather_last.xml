<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/match_weather_item_layout"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginRight="10dp"
    android:layout_marginTop="12dp"
    >

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/match_weather_last_text_button"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:text="사용자사용자"
        android:gravity="center"
        android:button="@color/transparent"
        android:textSize="14sp"
        android:paddingStart="20dp"
        android:paddingEnd="20dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:background="@drawable/radio_selector"
        android:textColor="@drawable/selector_radio_text"
        tools:ignore="MissingConstraints"
        />

    <ImageButton
        android:id="@+id/match_weather_last_delete_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_add_block_delete"
        android:background="#00ff0000"
        app:layout_constraintEnd_toEndOf="@id/match_weather_last_text_button"
        app:layout_constraintTop_toTopOf="@id/match_weather_last_text_button"
        app:layout_constraintBottom_toBottomOf="@id/match_weather_last_text_button"
        android:layout_marginEnd="8dp"
        android:visibility="gone"
        />




</androidx.constraintlayout.widget.ConstraintLayout>